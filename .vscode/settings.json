{
  // "rust-analyzer.cargo.noDefaultFeatures": true,
  // "rust-analyzer.cargo.features": ["default-dev"],
  "editor.formatOnSave": true,
  "rust-analyzer.check.allTargets": false,
  "rust-analyzer.linkedProjects": ["Cargo.toml"],
  "editor.tokenColorCustomizations": {
    "[One*]": {
      "comments": "#858585",
      "functions": "#31c3d6",
      "keywords": "#da914d",
      "numbers": "#c1bfc7",
      "strings": "#97be76",
      "types": "#d3b820",
      "textMateRules": [
        {
          "scope": "variable",
          "settings": {
            "foreground": "#c1bfc7",
            "fontStyle": ""
          }
        },
        {
          "scope": ["variable.other"],
          "settings": {
            "foreground": "#c1bfc7",
            "fontStyle": ""
          }
        },
        {
          "scope": "keyword.operator",
          "settings": {
            "foreground": "#31c3d6",
            "fontStyle": ""
          }
        },
        {
          "scope": "constant.other.caps",
          "settings": {
            "foreground": "#c58b64",
            "fontStyle": ""
          }
        },
        {
          "scope": "markup.underline",
          "settings": {
            "fontStyle": ""
          }
        },
        {
          "scope": "entity.name.function",
          "settings": {
            "fontStyle": ""
          }
        },
        {
          "scope": "keyword.other",
          "settings": {
            "foreground": "#e618a1"
          }
        },
        {
          "scope": "keyword.control",
          "settings": {
            "foreground": "#e618a1"
          }
        },
        {
          "scope": "entity.name.type.enum",
          "settings": {
            "foreground": "#1ca069"
          }
        },
        {
          "scope": "entity.name.type.struct",
          "settings": {
            "foreground": "#dabf26"
          }
        },
        {
          "scope": "entity.name.type.struct",
          "settings": {
            "foreground": "#dabf26"
          }
        },
        {
          "scope": "method",
          "settings": {
            "foreground": "#76a0ee",
            "fontStyle": ""
          }
        }
      ]
    },
    "comments": "",
    "textMateRules": []
  }
}
